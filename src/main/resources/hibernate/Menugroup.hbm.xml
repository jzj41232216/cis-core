<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sinosoft.cis.domain.model.user">
	<class
		name="Menugroup"
		table="menugroup"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="native"/>
		</id>

		<property
			name="Groupcode"
			column="groupcode"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Groupname"
			column="groupname"
			type="string"
			not-null="false"
			length="20"
		/>
	

		<set name="User2menugroups" inverse="true" lazy="true">
			<key column="id"/>
			<one-to-many class="User2menugroup"/>
		</set>

		<set name="Menugroup2menus" inverse="true" lazy="true">
			<key column="id"/>
			<one-to-many class="Menugroup2menu"/>
		</set>


	</class>	
</hibernate-mapping>